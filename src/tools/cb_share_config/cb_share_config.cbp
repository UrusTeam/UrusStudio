<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<UrusStudio_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Code::Blocks Share Config wx2.8.x" />
		<Option pch_mode="2" />
		<Option compiler="mingw32-gcc" />
		<Build>
			<Target title="default">
				<Option output="../../devel/cb_share_config.exe" prefix_auto="0" extension_auto="0" />
				<Option working_dir="../../devel" />
				<Option object_output="../../.objs/tools/cb_share_config" />
				<Option type="0" />
				<Option compiler="mingw32-gcc" />
			</Target>
			<Environment>
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-std=gnu++11" />
			<Add option='[[if (GetCompilerFactory().GetCompilerVersionString(_T(&quot;gcc&quot;)) &gt;= _T(&quot;4.8.0&quot;)) print(_T(&quot;-Wno-unused-local-typedefs&quot;));]]' />
			<Add option="-DUSE_PCH" />
			<Add option="-D__GNUWIN32__" />
			<Add option="-D__WXMSW__" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DWXUSINGDLL" />
			<Add directory="$(#WX.include)" />
			<Add directory="../../include" />
			<Add directory="../../include/tinyxml" />
			<Add directory="$(#wxsetup.include)" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX.include)" />
			<Add directory="$(#wxsetup.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-mthreads" />
			<Add library="txml" />
			<Add library="$(#wx.wxlibs)" />
			<Add directory="$(#WX.lib)" />
			<Add directory="../../base/tinyxml" />
			<Add directory="../../devel" />
		</Linker>
		<Unit filename="app.cpp" />
		<Unit filename="app.h" />
		<Unit filename="mainframe.cpp" />
		<Unit filename="mainframe.h" />
		<Unit filename="wxsmith/MainFrame.wxs" />
		<Extensions>
			<code_completion />
			<wxsmith version="1">
				<gui name="wxWidgets" src="app.cpp" main="MainFrame" init_handlers="necessary" language="CPP" />
				<resources>
					<wxFrame wxs="wxsmith/MainFrame.wxs" src="mainframe.cpp" hdr="mainframe.h" fwddecl="0" i18n="1" name="MainFrame" language="CPP" />
				</resources>
			</wxsmith>
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</UrusStudio_project_file>
